<table class="table table-condensed">
  <thead>
    <tr>
      <th>Username</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <% unless session[:username] == user.username %>
        <td><%= button_to "Destory User", @ticket, method: :delete, class: "btn btn-danger" %></td>
      <% else %>
        <td></td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>
