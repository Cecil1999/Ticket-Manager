<table class="table table-condensed">
  <thead>
    <tr>
      <th>User Email</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<div class="row">
  <div class="col">
    <%= link_to "Add User", new_user_registration_path, class: "btn btn-primary" %>
  </div>
</div>
