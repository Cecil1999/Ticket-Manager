<div class="row p-2">
  <ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item" role="presentation">
      <button class="nav-link active" id="user-tab" data-bs-toggle="tab" data-bs-target="#user-tab-pane" type="button" role="tab" aria-controls="user-tab-pane" aria-selected="true">Users</button>
    </li>
    <li class="nav-item" role="presentation">
      <button class="nav-link" id="ticket-tab" data-bs-toggle="tab" data-bs-target="#ticket-tab-pane" type="button" role="tab" aria-controls="ticket-tab-pane" aria-selected="false">Tickets</button>
    </li>
    <li class="nav-item" role="presentation">
      <button class="nav-link" id="ticket-type-tab" data-bs-toggle="tab" data-bs-target="#ticket-type-tab-pane" type="button" role="tab" aria-controls="ticket-type-tab-pane" aria-selected="false">Ticket Types</button>
    </li>
  </ul>
  <div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active p-1" id="user-tab-pane" role="tabpanel" aria-labelledby="user-tab" tabindex="0">
      <%= render 'pages/user-list' %>
    </div>
    <div class="tab-pane fade p-1" id="ticket-tab-pane" role="tabpanel" aria-labelledby="ticket-tab" tabindex="0">
      <%= render 'pages/ticket-list' %>
    </div>
    <div class="tab-pane fade p-1" id="ticket-type-tab-pane" role="tabpanel" aria-labelledby="ticket-type-tab" tabindex="0">
      <%= render 'pages/ticket-type-list' %>
    </div>
  </div>
</div>
